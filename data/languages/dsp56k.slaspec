macro acc_write_signed24(dst, src) {
  local tmp:3 = src;
  local tmp2:7 = sext(tmp);
  dst = tmp2 << 24;
}

macro acc_write_unsigned24(dst, src) {
  local tmp:3 = src;
  local tmp2:7 = zext(tmp);
  dst = tmp2 << 24;
}

macro acc_write_unsigned8(dst, src) {
  local tmp:1 = src;
  local tmp2:7 = zext(tmp);
  dst = tmp2 << 24;
}

macro stack_push(ophi, oplo) {
  local combined:6;
  combined[24,24]=ssh;
  combined[0, 24]=ssl;
  *[stackmem] sp = combined;
  sp = sp + 1;

  local t1:3=ophi;
  local t2:3=oplo;
  ssh=t1;
  ssl=t2;
}

macro stack_pop(ophi, oplo) {
  ophi = ssh;
  oplo = ssl;
  sp = sp - 1;
  local combined:6= *[stackmem] sp;
  ssh=combined[24,24];
  ssl=combined[0,24];
}

macro calc_cond(x, dst) {
  if
  ( ($(cf)==0 & x==0b0000)
  | ($(cf)==1 & x==0b1000)
  | ($(ef)==0 & x==0b0101)
  | ($(zf)==1 & x==0b1010)
  | ($(ef)==1 & x==0b1101)
  | (($(nf)^$(vf))==0 & x==0b0001)
  | (($(zf)+($(nf)^$(vf)))==0 & x==0b0111)
  | ($(lf)==0 & x==0b0110)
  | (($(zf)+($(nf)^$(vf)))==1 & x==0b1111)
  | ($(lf)==1 & x==0b1110)
  | (($(nf)^$(vf))==1 & x==0b1001)
  | ($(nf)==1 & x==0b1011)
  | ($(zf)==0 & x==0b0010)
  | ($(zf)+(!$(uf) & !$(ef))==1 & x==0b1100)
  | ($(nf)==0 & x==0b0011)
  | ($(zf)+(!$(uf) & !$(ef))==0 & x==0b0100)
  ) goto dst;
}

macro calc_cond_call(x, dst) {
  if (!
  ( ($(cf)==0 & x==0b0000)
  | ($(cf)==1 & x==0b1000)
  | ($(ef)==0 & x==0b0101)
  | ($(zf)==1 & x==0b1010)
  | ($(ef)==1 & x==0b1101)
  | (($(nf)^$(vf))==0 & x==0b0001)
  | (($(zf)+($(nf)^$(vf)))==0 & x==0b0111)
  | ($(lf)==0 & x==0b0110)
  | (($(zf)+($(nf)^$(vf)))==1 & x==0b1111)
  | ($(lf)==1 & x==0b1110)
  | (($(nf)^$(vf))==1 & x==0b1001)
  | ($(nf)==1 & x==0b1011)
  | ($(zf)==0 & x==0b0010)
  | ($(zf)+(!$(uf) & !$(ef))==1 & x==0b1100)
  | ($(nf)==0 & x==0b0011)
  | ($(zf)+(!$(uf) & !$(ef))==0 & x==0b0100)
  )) goto <calc_cond_call_end>;

  stack_push(inst_next, sr);
  call dst;
  <calc_cond_call_end>
}

# Workaround: feeding inst_next into the macro ain't work
macro calc_cond_inst_next(x) {
  if
  ( ($(cf)==0 & x==0b0000)
  | ($(cf)==1 & x==0b1000)
  | ($(ef)==0 & x==0b0101)
  | ($(zf)==1 & x==0b1010)
  | ($(ef)==1 & x==0b1101)
  | (($(nf)^$(vf))==0 & x==0b0001)
  | (($(zf)+($(nf)^$(vf)))==0 & x==0b0111)
  | ($(lf)==0 & x==0b0110)
  | (($(zf)+($(nf)^$(vf)))==1 & x==0b1111)
  | ($(lf)==1 & x==0b1110)
  | (($(nf)^$(vf))==1 & x==0b1001)
  | ($(nf)==1 & x==0b1011)
  | ($(zf)==0 & x==0b0010)
  | ($(zf)+(!$(uf) & !$(ef))==1 & x==0b1100)
  | ($(nf)==0 & x==0b0011)
  | ($(zf)+(!$(uf) & !$(ef))==0 & x==0b0100)
  ) goto inst_next;
}


macro calc_flags(mask, cbit, vbit, zbit, nbit, ubit, ebit, lbit, sbit) {
  local cf_cond:1 = mask & 0b00000001;
  local vf_cond:1 = mask & 0b00000010;
  local zf_cond:1 = mask & 0b00000100;
  local nf_cond:1 = mask & 0b00001000;
  local uf_cond:1 = mask & 0b00010000;
  local ef_cond:1 = mask & 0b00100000;
  local lf_cond:1 = mask & 0b01000000;
  local sf_cond:1 = mask & 0b10000000;

  if (cf_cond == 0) goto <post_cf>;
  $(cf)=cbit;
  <post_cf>
  if (vf_cond == 0) goto <post_vf>;
  $(vf)=vbit;
  <post_vf>
  if (zf_cond == 0) goto <post_zf>;
  $(zf)=zbit;
  <post_zf>
  if (nf_cond == 0) goto <post_nf>;
  $(nf)=nbit;
  <post_nf>
  if (uf_cond == 0) goto <post_uf>;
  $(uf)=ubit;
  <post_uf>
  if (ef_cond == 0) goto <post_ef>;
  $(ef)=ebit;
  <post_ef>
  if (lf_cond == 0) goto <post_lf>;
  $(lf)=lbit;
  <post_lf>
  if (sf_cond == 0) goto <post_sf>;
  $(sf)=sbit;
  <post_sf>
}

